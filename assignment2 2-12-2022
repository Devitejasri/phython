def vote():
    if v>=18:
     print('Yes')
    else:
     print('No')
print('OUYPUT 1\n')
v=int(input('Enter the age:'))
vote()


def vote():
    if v>=18:
        return'Yes'
    else:
        return'No'
print('\nOUTPUT 2\n')    
v=int(input('Enter the age:'))
print(vote())


def vote():
    if v>=18:
        return True
    else:
        return False
print('\nOUTPUT 3\n')
v=int(input('Enter the age:'))
if(vote()):
    print('Yes')
else:
    print('No')



